{
  "name": "aloo",
  "version": "0.1.0",
  "description": "Starting point with NodeJS, ExpressJS, SASS, ReactJS and Redux",
  "main": "app.js",
  "scripts": {
    "start-test-watch": "./node_modules/jest/bin/jest.js --watchAll",
    "start-client-watch": "NODE_ENV=development_client babel-node ./bin/app",
    "start-server-watch": "NODE_ENV=development_server ./node_modules/nodemon/bin/nodemon.js ./bin/app --exec babel-node",
    "build-server": "NODE_ENV=production babel . -d dist --ignore ./node_modules,./dist,./public,./scss --copy-files"
  },
  "author": "Mohammad Faisal",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.17.1",
    "express": "^4.15.2",
    "node-sass-middleware": "^0.11.0",
    "pug": "^2.0.0-beta11",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-promise": "^0.5.3"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "jest": "^19.0.2",
    "morgan": "^1.8.1",
    "nodemon": "^1.11.0",
    "regenerator-runtime": "^0.10.3",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1",
    "write-file-webpack-plugin": "^3.4.2"
  }
}
